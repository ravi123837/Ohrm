import cucumber.api.java.en.When;

public class Valid_Invalid_credentials {

	WebDriver driver=null;
	String url="https://opensource-demo.orangehrmlive.com/";


	
	driver.findElement(By.id("txtUsername")).sendKeys("Admin");
